{
  "name": "calendar-notifire",
  "version": "0.0.1",
  "description": "In future this will be a package to notifi channels like a telegram-bot in case of calendar events",
  "main": "index.ts",
  "scripts": {
    "test:watch": "jest --coverage --watchAll",
    "test": "jest --coverage",
    "build": "pretty-quick && npm run check && rimraf ./build && tsc -p ./tsconfig.build.json",
    "start:dev": "nodemon",
    "start": "npm run build && node build/index.js",
    "check": "./node_modules/.bin/tslint --project \".\" --exclude '**/*.d.ts' ",
    "pretty": "pretty-quick",
    "precommit": "pretty-quick --staged && lint-staged",
    "prepush": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deeKay93/calendar-notifire.git"
  },
  "keywords": [
    "Calendar",
    "Notification",
    "Telegram",
    "Events"
  ],
  "author": "daniel.knecht@live.de",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/deeKay93/calendar-notifire/issues"
  },
  "homepage": "https://github.com/deeKay93/calendar-notifire#readme",
  "devDependencies": {
    "@types/jest": "^24.9.0",
    "@types/node": "^13.1.6",
    "husky": "^4.0.4",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "nock": "^11.7.2",
    "nodemon": "^2.0.2",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.0",
    "source-map-support": "^0.5.16",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-no-unused-expression-chai": "^0.1.4",
    "typescript": "^3.7.4"
  },
  "jest": {
    "roots": [
      "src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,js}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit",
      "pre-push": "npm run prepush"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run check"
    ]
  },
  "dependencies": {
    "axios": "^0.19.1",
    "ical-expander": "^2.1.0"
  }
}
